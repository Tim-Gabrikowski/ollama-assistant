{
	"name": "ollama-assistant",
	"version": "0.1.0",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Tim-Gabrikowski/ollama-assistant.git"
	},
	"scripts": {
		"build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
		"watch": "tsc -w --project tsconfig.json",
		"watch:alias": "tsc-alias -w -p tsconfig.json",
		"dev": "concurrently -k -n TSC,ALIAS -c green,blue \"npm:watch\" \"npm:watch:alias\""
	},
	"keywords": [
		"ollama",
		"assistant",
		"tool",
		"wrapper",
		"ai"
	],
	"author": "Tim Gabrikowski",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^22.8.4",
		"concurrently": "^9.1.2",
		"tsc-alias": "^1.8.10",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.19.2",
		"typescript": "^5.6.3"
	},
	"dependencies": {
		"chalk": "^5.4.1",
		"ollama": "^0.5.15"
	}
}
